<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- Square Background -->
    <item>
        <shape
            android:shape="rectangle">
            <corners android:radius="8dp" />
            <stroke
                android:color="#000"
            android:dashWidth="4dp"
            android:dashGap="4dp"
            android:thickness="1dp"
            />
        </shape>
    </item>
</layer-list>
